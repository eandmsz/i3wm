*background: #002b36
*foreground: #657b83
!!*fading: 40
*fadeColor: #002b36
*cursorColor: #93a1a1
*pointerColorBackground: #586e75
*pointerColorForeground: #93a1a1

!! black dark/light
*color0: #073642
*color8: #002b36

!! red dark/light
*color1: #dc322f
*color9: #cb4b16

!! green dark/light
*color2: #449307
*color10: #56B30B

!! yellow dark/light
*color3: #b58900
*color11: #657b83

!! blue dark/light
*color4: #268bd2
*color12: #839496

!! magenta dark/light
*color5: #d33682
*color13: #6c71c4

!! cyan dark/light
*color6: #2aa198
*color14: #93a1a1

!! white dark/light
*color7: #eee8d5
*color15: #fdf6e3

! Save number lines in the scrollback buffer [default 1000]; option -sl.
URxvt.saveLines: 25000

! True: blink the cursor. False: do not blink the cursor [default]; option -bc.
URxvt.cursorBlink: True

! Nice Fonts
URxvt.font:xft:Monospace:pixelsize=12
URxvt.boldfont:xft:Monospace-Bold:pixelsize=12

! Hide Scrollbar
URxvt.scrollBar: False

! Perl extensions (font-size needs the urxvt-font-size package)
URxvt.perl-ext-common:      default,selection-to-clipboard

! Turn on/off ISO 14755 (default enabled). This is to be Unicode compliant.
URxvt.iso14755: False
URxvt.iso14755_52: False
